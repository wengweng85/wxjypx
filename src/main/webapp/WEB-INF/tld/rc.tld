    <!-- select -->
    <tag>  
	    <name>select</name>  
	    <tag-class>com.insigma.tag.form.input.SelectTag</tag-class>  
	    <description>input输入框之select标签</description>
	   
	    <attribute>
	        <description>属性名:对应原生input框中的id和name.id与name名称一致</description>
	        <name>property</name>
	        <required>true</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>input输入框对应label名</description>
	        <name>label</name>
	        <required>true</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>input输入对应自适应列数,形如1,3逗号前代表列占位列数据,逗号后代码输入框占位列数。规则采用bootstrap的栅栏系统,形如1,2</description>
	        <name>cols</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>代码类型:从codevalue中获取数据的数据类型;如codetype="AAC04"性别</description>
	        <name>codetype</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>代码过滤条件,形如aaa102='2' and aaa103='姓名'</description>
	        <name>filter</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	     <attribute>
	        <description>是否多选</description>
	        <name>multiple</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>显示列数</description>
	        <name>size</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	     <attribute>
	        <description>基于jquery validator的检验js.形如 validate="{required:true,messages:{required:'请输入权限编码'}}",js规则可参考jquery validator插件</description>
	        <name>validate</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>值：input值</description>
	        <name>value</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>是否必输</description>
	        <name>required</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>是否只读</description>
	        <name>readonly</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>onchange事件函数,形如onchange="aac003_change(this)"</description>
	        <name>onchange</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>onclick事件函数</description>
	        <name>onclick</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>onblur事件函数</description>
	        <name>onblur</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>onkeypress事件函数</description>
	        <name>onkeypress</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>onkeydown事件函数</description>
	        <name>onkeydown</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	        <description>onkeyup事件函数</description>
	        <name>onkeyup</name>
	        <required>false</required>
	        <rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <body-content>empty</body-content>  
    </tag>  
    
